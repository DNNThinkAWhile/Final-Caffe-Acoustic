
name: "Acoustic"
layer {
  name: "data"
  type: "Data"
  top: "data"
  top: "label"
  include {
    phase: TRAIN
  }
  transform_param {
    scale: 1
  }
  data_param {
    source: "/tmp2/weitang114/tang/Final-Caffe-Acoustic/train_lmdb"
    batch_size:256
    backend: LMDB
    scale: 1
  }
}
layer {
  name: "data"
  type: "Data"
  top: "data"
  top: "label"
  include {
    phase: TEST
  }
  transform_param {
    scale: 1
  }
  data_param {
    source: "/tmp2/weitang114/tang/Final-Caffe-Acoustic/test_lmdb"
    batch_size: 256
    backend: LMDB
    scale: 1
  }
}
layer {
  name: "fc1"
  type: "InnerProduct"
  bottom: "data"
  top: "fc1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 1024
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu1"
  bottom: "fc1"
  top: "fc1"
  type: "ReLU"
}
